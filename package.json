{
  "name": "topln-ru-test-task",
  "version": "0.1.0",
  "description": "Test task for full stack JS developer position in the company topln.ru",
  "main": "source/index.js",
  "scripts": {
    "dev:test": "jest --watch",
    "dev:ui": "nodemon ./source/index.js & webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "start": "node source/index.js",
    "lint": "eslint *.js {source,migrations}/**/*.{vue,js}",
    "test": "npm run lint && jest --coverage"
  },
  "repository": "nikolay-govorov/topln.ru-test-task",
  "author": "Nikolay Govorov <nikolay_govorov@bk.ru>",
  "license": "UNLICENSE",
  "homepage": "https://topln-ru.herokuapp.com",
  "dependencies": {
    "axios": "^0.18.0",
    "convict": "^4.0.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.9",
    "jest": "^22.4.0",
    "mysql": "^2.15.0",
    "pug": "^2.0.0-rc.4",
    "v-calendar": "^0.6.3",
    "vue": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-vue": "^4.2.2",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.19",
    "postcss-loader": "^2.1.0",
    "postcss-scss": "^1.0.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended",
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "jest": true,
      "browser": true
    },
    "rules": {
      "no-param-reassign": "off",
      "indent": "off",
      "indent-legacy": [
        "error",
        2
      ],
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1,
          "switchCase": 0,
          "ignores": []
        }
      ]
    }
  }
}
